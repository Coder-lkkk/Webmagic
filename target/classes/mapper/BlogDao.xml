<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lkkk.dao.CsdnBlogDao" >
    <insert id="insertBlog">
        INSERT  INTO csdnblog(title,publishTime,tag,specialColumn,author,sourceUrl,readNum)
        VALUES (#{blog.title},#{blog.publishTime},#{blog.tag},#{blog.specialColumn},#{blog.author},#{blog.sourceUrl},#{blog.readNum})
    </insert>

</mapper>